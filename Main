public class MainPet {
    public static void main(String[] args) {
               
        Pet pet = new Cat();
        pet.setName("Vovuri");
        pet.getName();
        pet.play();

        Cat cat = new Cat();
        cat.setName("Garfield");
        cat.getName();
        cat.play();
        cat.eat();

        Fish fish = new Fish();
        fish.setName("Nemo");
        fish.getName();
        fish.play();
        fish.walk();
        fish.eat();
        
    }
    }
